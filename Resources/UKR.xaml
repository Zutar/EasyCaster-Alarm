    <system:String x:Key="settingsKeyTitle2">Ключова фраза #2</system:String>
    <system:String x:Key="settingsKeyTitle3">Ключова фраза #3</system:String>
    <system:String x:Key="settingsKeyTitle4">Ключова фраза #4</system:String>
    <system:String x:Key="settingsKeyApp1">Додаток #1</system:String>
    <system:String x:Key="settingsKeyApp2">Додаток #2</system:String>
    <system:String x:Key="settingsKeyApp3">Додаток #3</system:String>
